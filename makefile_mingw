endif
ifeq ($(MODULE_QTCORE),yes)
   ALLLIBS += $(QT4XHB_LIB_DIR)/libhbqtcore4$(LIBEXT)
endif
ifeq ($(MODULE_QTDECLARATIVE),yes)
   ALLLIBS += $(QT4XHB_LIB_DIR)/libhbqtdeclarative4$(LIBEXT)
endif
ifeq ($(MODULE_QTDESIGNER),yes)
   ALLLIBS += $(QT4XHB_LIB_DIR)/libhbqtdesigner4$(LIBEXT)
endif
ifeq ($(MODULE_QTGUI),yes)
   ALLLIBS += $(QT4XHB_LIB_DIR)/libhbqtgui4$(LIBEXT)
