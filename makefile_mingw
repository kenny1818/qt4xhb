   include mak/QtUiToolsMocs.mak
endif
ifeq ($(MODULE_QTWEBKIT),yes)
   include mak/QtWebKitMocs.mak
endif
ifeq ($(MODULE_QTXML),yes)
   include mak/QtXmlMocs.mak
endif
